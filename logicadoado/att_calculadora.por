programa {
  funcao inicio() {
    real numero1, numero2

		escreva("Digite um número: ")
		leia(numero1)

		escreva("Digite o segundo número: ")
		leia (numero2)

		escreva("A soma dos dois número é: ", numero1+numero2, "\n")
		escreva("A subtração dos dois número é: ", numero1-numero2, "\n")
		escreva("A divisão dos dois número é: ", numero1/numero2, "\n")
		escreva("A multiplicação dos dois número é: ", numero1*numero2, "\n")
	} 
}



